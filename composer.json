{
    "name": "sonata-project/composer-archive-creator",
    "type": "project",
    "description": "Create archives of a composer.json for latter use in order to speed up install process",
    "keywords": ["composer"],
    "homepage": "http://sonata-project.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        }
    ],
    "require": {
        "symfony/process": "~2.3",
        "symfony/console": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/filesystem": "~2.3",
        "swiftmailer/swiftmailer": "~5.0",
        "symfony/config": "~2.3",
        "symfony/finder": "~2.3"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
